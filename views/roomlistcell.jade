#roomlistCell-template(style="display:none;")
	table#celltemplate-table
		tr.room-row
			td.room-name
				.room-name-container
					.listed-room-name {{ roomname }}
						img.valstream-icon(src="images/room/icons/gmail.png")
					.true-room-name {{ truename }}
			td.room-history
				ul.room-history-container
				.lastPlayedVideoTitle
			td.room-friends
				{{ numDJs }} /
				{{ viewers }} /
				.room-friends-container {{ partial_friends }}
				ul.friends-hover-container {{ full_friends }}