	#rooms
		width: 600px
		height: 310px
		overflow-y: scroll
		overflow-x: hidden
		table#roomsTable
			width: 590px
			border-collapse: collapse
			tr.table-header
				background-color: black
				color: white
				height: 30px
				th.viewers-hd
					width: 50px
					padding: 6px
