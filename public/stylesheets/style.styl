//Chat Box
div.chatContainer
	font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif
	width: 300px
	height: 296px
	max-width: 300px
	max-height: 290px
	display: block
	div.chatHeader
		width: 300px
		height: 30px
		background-image: url('/images/room/actionbar.png')
		background-color: gray
		img.chatIcon
			display: inline-block
			margin-top: 5px
			margin-left: 5px
		div.chatTitle
			text-indent: 10px
			vertical-align: top
			display: inline-block                                         
			font-weight: 500
			color: white
			height: 18px
			width: 36px
	#messages
		background-color: #E7E7E7
		border: none
		color: black
		height: 230px
		overflow-y: scroll
		overflow-x: hidden
		div.messageContainer
			padding: 2px 4px
			min-height: 20px
			height: auto
			overflow-x: hidden
			overflow-y: visible;
			width: 274px
			max-width: 274px
			font-size: 14px
			color: black
			span
				padding: 2px 0px 2px
				margin-right: 3px
				font-weight: bold
				display: inline-block
			div
				padding-top: 2px
				padding-bottom: 2px
				display: inline
		div.messageContainer:nth-child(odd)
					background-color: white
	form.inputBox
		background-image: url('/images/room/search.png')
		height: 36px
		text-indent: 30px
		font-size: 14px
		input
			margin-top: 10px
			width: 240px
			height: 14px
			border: 0
			outline: 0
			background: transparent
					 
