#room-modal
	font-family: "HelveticaNeue-Light", "Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif
	overflow-y: none
	overflow-x: none
	width: 600px
	height: 360px
	border: 1px solid black
	#searchbar
		background: url('http://i.imgur.com/4QNd7.png') 50% 50% no-repeat
		display: block
		height: 50px
		background-color: #666
		form.roomSearch
			display: inline-block
			width: 560px
			height: 40px
			text-indent: 45px
